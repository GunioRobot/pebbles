<tr valign="top">

<td><%= set_decimal(store.distance)%></td>
 
 <td><a href = "<%= store.url%>"><h4><%= store.storename %></h4></a><br>
      <%= store.address %><br>
      <%= store.city %>, <%= store.state %>, <%= store.zip %>, <%= store.country.upcase %><br>
      <%= store.phone %>
 </td>
 <% unless store.store_image == nil %>
 <td><%= image_tag store.store_image, :name => "image_id" %></td>
  <% end %>
</tr>
