<%= javascript_tag "window.AUTH_TOKEN = '#{form_authenticity_token}'" %>

<h1>Categories</h1>

<ul id="category-list">
  <% @categories.each do |category| %>
    <li class="category_item" id="category=<%= category.id %>">
      <%=link_to(category.name, admin_category_path(category)) %>
    </li>
  <% end %>
</ul>
